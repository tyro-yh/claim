<mapper namespace="com.claim.server.datacollection.dao.BFileListDao">
    <select id="selectByCollectionId" parameterType="integer" resultType="com.claim.server.datacollection.po.BFileList">
        SELECT * FROM b_file_list WHERE CollectionId = #{collectionId}
    </select>
</mapper>