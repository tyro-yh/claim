<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- ============================================================== -->
<!-- ================可直接使用Base配置文件中定义的节点！================ -->
<!-- ============================================================== -->
<mapper namespace="com.claim.server.datacollection.dao.BDataCollectionDao">
    <select id="selectByReportNoAndCertifyCode" parameterType="string" resultType="com.claim.server.datacollection.po.BDataCollection">
        SELECT * FROM b_data_collection WHERE ReportNo = #{reportNo} AND CertifyCode = #{certifyCode}
    </select>
</mapper>